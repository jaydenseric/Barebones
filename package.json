{
  "name": "barebones",
  "version": "4.0.0-beta.2",
  "description": "A barebones framework for getting started on a modern front end.",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "http://jaydenseric.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenseric/Barebones"
  },
  "bugs": {
    "url": "https://github.com/jaydenseric/Barebones/issues"
  },
  "private": true,
  "scripts": {
    "lint": "eslint . --ignore-pattern bundle || true",
    "clean": "rm -r bundle || true",
    "prebuild": "npm run clean",
    "build": "webpack -p --progress",
    "build:watch": "npm run build -- --watch",
    "start": "http-server -o"
  },
  "dependencies": {
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "dom-elements": "^0.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-reporter": "^1.3.3",
    "style-loader": "^0.13.0",
    "svgxuse": "^1.1.10",
    "webpack": "^2.1.0-beta.4",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "http-server": "^0.9.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "rules": {
      "no-new": 1,
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    },
    "globals": {
      "fetch": false
    }
  }
}
