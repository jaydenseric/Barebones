{
  "name": "barebones",
  "version": "3.0.4",
  "description": "A barebones framework for getting started on a modern front end.",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "http://jaydenseric.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenseric/Barebones"
  },
  "bugs": {
    "url": "https://github.com/jaydenseric/Barebones/issues"
  },
  "private": true,
  "scripts": {
    "lint": "eslint . || true",
    "task": "gulp",
    "start": "http-server -o"
  },
  "dependencies": {
    "cssnano": "^3.5.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-concat": "^2.6.0",
    "gulp-postcss": "^6.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "postcss-cssnext": "^2.4.0",
    "postcss-reporter": "^1.3.3"
  },
  "devDependencies": {
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "http-server": "^0.9.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    }
  }
}
